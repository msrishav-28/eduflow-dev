{
  "name": "eduflow-landing",
  "version": "1.0.0",
  "description": "EduFlow - AI-powered education platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cd backend && python server.py\" \"cd frontend && npm start\"",
    "frontend:dev": "cd frontend && npm start",
    "backend:dev": "cd backend && python server.py",
    "frontend:build": "cd frontend && npm run build",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "frontend:install": "cd frontend && npm install --legacy-peer-deps",
    "install-all": "npm run frontend:install && npm run backend:install",
    "test": "cd frontend && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "education",
    "ai",
    "learning",
    "react",
    "fastapi",
    "mongodb"
  ],
  "author": "EduFlow Team",
  "license": "MIT"
}
